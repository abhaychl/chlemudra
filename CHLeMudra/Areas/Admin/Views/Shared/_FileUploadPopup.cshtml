@model CHLeMudra.Web.Models.FileUploadModel
<a href="#FileModal" id="OpenFilePoup" data-toggle="modal" class="btn btn-success" style="display:none;">View Popup</a>
<div id="FileModal" class="modal hide">
    <div class="modal-header">
        <button data-dismiss="modal" class="close" type="button">x</button>
        <h3>Upload File</h3>
    </div>
    <div class="modal-body">

        <div class="widget-content nopadding" style="text-align:left;">

            <div class="row-fluid">
                <div class="control-group span3">
                    <label>Upload File</label>
                </div>
                <div class="control-group span9" >
                    <div class="controls">
                        @Html.EditorFor(model => model.UploadFile)
                    </div>
                </div>
            </div>
       
            
            <div class="row-fluid">
                <div class="control-group span3">
                    <div>&nbsp;</div>
                </div>
                <div class="control-group span2">
                    <div class="controls">
                        <input type="hidden" id="hdnBookingNo" name="hdnBookingNo" />

                        <button id="btnfileSubmit" onclick="" class="btn btn-success">Submit</button>
                    </div>
                </div>
                <div class="control-group span1">
                    <div class="controls">
                        <button class="btn btn-info btnCancel btnCancel">Cancel</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>